{
  "private": true,
  "name": "helloworld-express",
  "version": "0.1.0",
  "engines": {
    "node": "^16.0.0",
    "npm": "^8.0.0"
  },
  "scripts": {
    "start": "npm run app",
    "dev": "nodemon -r esm --inspect --inspect-port=5900 ./app-all.js",
    "dev:core": "nodemon -r esm --inspect --inspect-port=5901 ./app-core.js",
    "dev:api": "nodemon -r esm --inspect --inspect-port=5902 ./app-api.js",
    "dev:job": "nodemon -r esm --inspect --inspect-port=5903 ./app-job.js",
    "dev:console": "node -r esm ./app-console.js",
    "run-script": "node -r esm",
    "data:clear": "node -r esm ./scripts/data-clear.js;",
    "data:seed": "node -r esm ./scripts/data-seed.js;",
    "format": "prettier --cache --list-different --write '**/.*.{yml,json,js}' '**/*.{md,yml,json,ts,html,css}' ; npm run lint -- --quiet --fix",
    "lint": "eslint --cache ./",
    "typecheck": "tsc -p jsconfig.json",
    "test": "jest --runInBand",
    "test:unit": "jest --runInBand \\.test\\.js",
    "test:spec": "jest --runInBand \\.spec\\.js",
    "docs": "npm run docs:jsdoc ; npm run docs:apib ; npm run docs:postman",
    "docs:jsdoc": "jsdoc -c ./jsdoc.json",
    "docs:apib": "mkdir -p ./generated/apib ; docker run -it --rm -u $(id -u) -v $PWD:/data emiketic/development aglio --theme-variables flatly -o /data/generated/apib/index.html -i /data/docs/api/index.apib",
    "docs:postman": "mkdir -p ./generated/postman ; docker run -it --rm -u $(id -u) -v $PWD:/data emiketic/development sh -c 'cat $(find /data/ -name *.apib) | apiary2postman blueprint > /data/generated/postman/API.postman_collection.json'"
  },
  "dependencies": {
    "~": "./src",
    "bcrypt": "~5.1.0",
    "bull": "~3.22.8",
    "debug": "~4.3.4",
    "dotenv": "~10.0.0",
    "ejs": "~3.1.8",
    "esm": "~3.2.25",
    "events": "~3.3.0",
    "express": "~4.18.2",
    "express-async-errors": "~3.1.1",
    "express-graphql": "~0.12.0",
    "formdata-node": "~3.5.4",
    "glob": "~7.1.7",
    "graphql": "~15.5.1",
    "jsonwebtoken": "~8.5.1",
    "juice": "~8.1.0",
    "morgan": "~1.10.0",
    "node-fetch": "~2.6.1",
    "node-schedule": "~2.1.0",
    "notifme-sdk": "~1.11.0",
    "redis": "~3.1.2",
    "sails-disk": "~2.1.2",
    "sails-mongo": "~2.0.0",
    "uuid": "~8.3.2",
    "waterline": "~0.13.6"
  },
  "devDependencies": {
    "@babel/core": "~7.19.3",
    "@babel/preset-env": "~7.19.4",
    "@types/express": "~4.17.14",
    "@types/glob": "~7.1.3",
    "@types/jest": "~26.0.23",
    "@types/node": "~15.6.0",
    "babel-eslint": "~10.1.0",
    "babel-jest": "~26.6.3",
    "eslint": "~7.27.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~3.4.0",
    "jest": "~26.6.3",
    "jsdoc": "~3.6.11",
    "nodemon": "~2.0.20",
    "prettier": "~2.7.1",
    "whatwg-fetch": "~3.6.2"
  }
}
